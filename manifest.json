{
    "manifest_version": 3,
    "name": "Focii Extension",
    "version": "1.0.0",
    "description": "Procrastination blocker",
    "permissions": [
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/focii.png"
    }
  }